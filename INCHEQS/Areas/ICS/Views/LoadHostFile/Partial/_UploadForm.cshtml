@using INCHEQS.Resources
@using (Html.BeginForm("UploadMultiple", "LoadHostFile", null, FormMethod.Post, new { @class = "formUploadFileMultiple", enctype = "multipart/form-data" })) {

    <input type="hidden" id="clearDate" name="clearDate" />

    <div id="manualUploadContainer" class="well white-bg hidden">
        <div class="form-group">
            <input type="hidden" id="fileTypeFormat" value="MTF" />
            <div id="uploadField">
                <input class="fileUploader" type="file" id="file" name="file" style="margin-left:15px" multiple/>
            </div>
            <hr />
            <button id="upload" type="submit" class="btn btn-default btn-xs" data-action="">@Locale.UploadFile</button>
        </div>
    </div>
}
