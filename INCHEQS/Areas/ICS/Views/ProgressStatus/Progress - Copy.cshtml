    var TotalPPS1stLevel = parseInt($("#Band0PPS1stLevel").text()) + parseInt($("#Band1PPS1stLevel").text()) +
        parseInt($("#Band2PPS1stLevel").text()) + parseInt($("#Band3PPS1stLevel").text())
