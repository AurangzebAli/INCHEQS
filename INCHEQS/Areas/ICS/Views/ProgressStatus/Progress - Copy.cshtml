        + parseInt($("#Band4_PendingBranch").text()) + parseInt($("#Band5_PendingBranch").text());

    var TotalAll = parseInt($("#Band0Total").text()) + parseInt($("#Band1Total").text()) +
        parseInt($("#Band2Total").text()) + parseInt($("#Band3Total").text())
        + parseInt($("#Band4Total").text()) + parseInt($("#Band5Total").text());

    var TotalPullOut = parseInt($("#Band0_PullOut").text()) + parseInt($("#Band1_PullOut").text()) +
        parseInt($("#Band2_PullOut").text()) + parseInt($("#Band3_PullOut").text())
        + parseInt($("#Band5_PullOut").text()) + parseInt($("#Band4_PullOut").text());
    
    $("#TotalRRChecker").text(TotalRRChecker);
    $("#TotalRR").text(TotalRR);
    $("#TotalCompleted").text(TotalCompleted);
    $("#TotalCompletedApproved").text(TotalCompletedApproved);
    $("#TotalCompletedRejected").text(TotalCompletedRejected);
    $("#Total1stLevel").text(Total1stLevel);
    $("#Total2ndLevel").text(Total2ndLevel);
    $("#TotalPPS1stLevel").text(TotalPPS1stLevel);
    $("#TotalPPS2ndLevel").text(TotalPPS2ndLevel);
    $("#TotalPendingBranch").text(TotalPendingBranch);
