        parseInt($("#Band4RR").text()) + parseInt($("#Band3RR").text()) +
        parseInt($("#Band2RR").text()) + parseInt($("#Band1RR").text())
        + parseInt($("#Band0RR").text());

    var TotalRRChecker = parseInt($("#Band5RRChecker").text()) +
