@using INCHEQS.Resources
<table class="table table-bordered mTop10">
	<thead>
		<tr>
            <th>Cheque Digit</th>
			<th>@Locale.ChequeSerialNo</th>
            <th>@Locale.BankCode</th>
            <th>@Locale.BranchCode</th>
			<th>@Locale.StateCode</th>
			<th>@Locale.AccountNumber</th>
			<th>@Locale.Transcode</th>
            <th>@Locale.Amount</th>
            <th>@Locale.Remarks</th>
            @*<th>TimeStamp</th>*@

		</tr>
	</thead>
	<tbody>
        @*<tr>
            <td><span>@ViewBag.ChequeHistory.getField("fldcheckdigit")</span></td>
            <td><span>@ViewBag.ChequeHistory.getField("fldserial")</span></td>
            <td><span>@ViewBag.ChequeHistory.getField("fldbankcode")</span></td>
            <td><span>@ViewBag.ChequeHistory.getField("fldbranchcode")</span></td>
            <td><span>@ViewBag.ChequeHistory.getField("fldstatecode")</span></td>
            <td><span>@ViewBag.ChequeHistory.getField("fldissueraccno")</span></td>
            <td><span>@ViewBag.ChequeHistory.getField("fldtccode")</span></td>
            <td><span>@ViewBag.ChequeHistory.getField("fldamount")</span></td>
            <td><span>@ViewBag.ChequeHistory.getField("fldremark")</span></td>
        </tr>*@
    @if (ViewBag.ChequeRepair != null ) //NUll check for Model
    {
            foreach (var row in ViewBag.ChequeRepair)
            {
                <tr>
                    <td><span>@row.fldCheckDigit</span></td>
                    <td><span>@row.fldSerial</span></td>
                    <td><span>@row.fldBankCode</span></td>
                    <td><span>@row.fldBranchCode</span></td>
                    <td><span>@row.fldStateCode</span></td>
                    <td><span>@row.fldIssuerAccNo</span></td>
                    <td><span>@row.fldTCCode</span></td>
                    <td td align="right"><span>@row.fldAmount</span></td>
                    <td><span>@row.fldRemark</span></td>
                    @*<td><span>@row.fldCreateTimeStamp</span></td>*@
                </tr>
            }
    }
	</tbody>
</table>